{
  "name": "dprint-plugin-prettier",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "build:script": "parcel build"
  },
  "source": "./main.ts",
  "main": "./dist/startup.js",
  "type": "module",
  "targets": {
    "main": false,
    "default": {
      "context": "browser",
      "includeNodeModules": true,
      "optimize": false,
      "sourceMap": false,
      "outputFormat": "global",
      "distDir": "./dist"
    }
  },
  "browser": {
    "url": "./shims/url.js",
    "process": "process/browser",
    "buffer": "./node_modules/buffer"
  },
  "engines": {
    "chrome": "58"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dprint/dprint-plugin-prettier.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dprint/dprint-plugin-prettier/issues"
  },
  "homepage": "https://github.com/dprint/dprint-plugin-prettier#readme",
  "dependencies": {
    "buffer": "^6.0.3",
    "prettier": "^3.7.4",
    "prettier-plugin-jsdoc": "^1.8.0",
    "prettier-plugin-svelte": "^3.4.0",
    "process": "^0.11.10",
    "url": "^0.11.4"
  },
  "devDependencies": {
    "@types/node": "^22",
    "crypto-browserify": "^3.12.1",
    "events": "^3.3.0",
    "parcel": "^2.13.3",
    "stream-browserify": "^3.0.0",
    "typescript": "^5.7.2",
    "util": "^0.12.5"
  }
}
